<h1>Listing rentable_rooms</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Start</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @rentable_rooms.each do |rentable_room| %>
      <tr>
        <td><%= rentable_room.value %></td>
        <td><%= rentable_room.start %></td>
        <td><%= link_to 'Show', rentable_room %></td>
        <td><%= link_to 'Edit', edit_rentable_room_path(rentable_room) %></td>
        <td><%= link_to 'Destroy', rentable_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

